@page "/new-bug"
<h3>New Bug</h3>

<EditForm Model="AddBug">
    <ValidationSummary />
    <DataAnnotationsValidator/>
    <input type="text" @bind-value="AddBug.Title" placeholder="Enter Description" />
    <input type="number" @bind-value="AddBug.Priority" placeholder="Enter Priority" />
    <button type="submit">Add Bug</button>
</EditForm>